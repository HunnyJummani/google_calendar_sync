<%= form_for(session, url: event_sessions_path) do |f| %>
  <%= render 'shared/error_messages', resource: session %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>
  <br><br>

  <%= f.label :start_time, "Start time" %>
  <%= f.datetime_field(:start_time) %>


  <%= f.label :end_time, "End time" %>
  <%= f.datetime_field(:end_time) %>
  <br><br>

  <%= f.label :attendees, "Attendees" %>
  <%= f.select :attendees, options_for_select(user_emails), {include_hidden: false}, { multiple: true, size: 5} %>
  <br><br>

  <%= f.submit %>

<% end %>